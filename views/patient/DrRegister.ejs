<% layout("/layouts/boilerplate") %>
<body>
    <div class="row mt-3">
        <!-- <h3 class="col-8 offset-2">Login on DocMitra</h3> -->
        <div class="col-8 offset-2">
            <form method="POST" action="/DocMitra/register/Doctor_register" novalidate class="needs-validation"/>
    
                <div class="mb-3">
                    <label for="username" class="form-label">Doctor Name</label>
                    <input 
                     type="text" 
                     name="username"
                     id="username"
                     class="form-control" required>
    
                </div> 
                <div class="mb-3">
                    <label for="Mobile" class="form-label">Mobile No.</label>
                    <input 
                     type="Number" 
                     name="Mobile"
                     id="Mobile"
                     class="form-control" required>
    
                        
                <div class="mb-3">
                    <label for="password" class="form-label">password</label>
                    <input 
                     type="password" 
                     name="password"
                     id="password"
                     class="form-control" required>
    
                     
                </div>
                <button class="btn btn-success">Login</button>
                </form>
         </div>
    </div>
</body>
